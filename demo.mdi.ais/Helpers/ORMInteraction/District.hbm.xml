<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="demo.mdi.ais" namespace="demo.mdi.ais.Helpers.ORMInteraction">
  <class name="District" table="[Районы доставки]" dynamic-update="true" >
    <cache usage="read-write"/>
    
    <id column="[№ Района]" type="int" name="DistrictNumber" />
    <property column="Название" type = "string" name = "Name" />
    <property column="[Количество обслуживаемых ТТ]" type="int" name="SalesPointCount" />
    
    <set name ="SalesPoints" lazy="true">
      <key column ="[№ Района]"/>
      <one-to-many class ="District"/>
    </set>
    <set name ="DistanceBetweenSalesPoints" lazy="true">
      <key column ="[№ Района]"/>
      <one-to-many class ="District"/>
    </set>
    <!--<set name ="TotalDeliveryVolumes" lazy="true">
      <key column ="[№ Района]"/>
      <one-to-many class ="District"/>
    </set>-->
  </class>
</hibernate-mapping>